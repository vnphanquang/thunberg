{
  "name": "@vnphanquang/pqsudoku-web",
  "version": "0.1.0",
  "private": true,
  "description": "pqsudoku webapp",
  "author": "Quang Phan",
  "type": "module",
  "scripts": {
    "lint:es": "eslint src --ext .ts,.tsx",
    "lint:style": "stylelint ./src/**/*.{svelte,css,postcss}",
    "format:es": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "dev": "cross-env NODE_ENV=development svelte-kit dev",
    "start": "node ./build/index.js",
    "build:dev": "cross-env NODE_ENV=development svelte-kit build",
    "build:prod": "cross-env NODE_ENV=production svelte-kit build",
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "preview": "svelte-kit preview",
    "test": "echo 'no implemented tests'",
    "codegen:svg": "node ./scripts/svgToSvelte.mjs",
    "codegen:routing": "ts-node --script-mode ./scripts/routing/index.ts && prettier ./src/lib/generated/routing/ --write",
    "codegen": "run-p codegen:*"
  },
  "dependencies": {
    "@vnphanquang/common": "workspace:*",
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0-next.53",
    "@sveltejs/kit": "1.0.0-next.180",
    "@vnphanquang/eslint-config": "workspace:*",
    "@vnphanquang/tailwind": "workspace:*",
    "@types/node": "^16.11.6",
    "@types/tailwindcss": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "autoprefixer": "^10.3.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-svelte3": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.1",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-fa": "^2.3.3",
    "svelte-preprocess": "^4.9.8",
    "svg-to-svelte": "^2.2.1",
    "tailwindcss": "^2.2.17",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
