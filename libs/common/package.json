{
  "name": "@thunberg/common",
  "version": "0.1.0",
  "private": true,
  "description": "Common goodies for use across projects",
  "author": "Quang Phan",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./env": {
      "import": "./dist/esm/env.js",
      "require": "./dist/cjs/env.js"
    },
    "./utils": {
      "import": "./dist/esm/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    }
  },
  "types": "./dist/types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "env": [
        "./dist/types/src/constants/env.d.ts"
      ],
      "utils": [
        "./dist/types/src/utils/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint:es": "eslint src --ext .ts,.tsx",
    "build": "rollup -c",
    "test": "echo 'no implemented tests'"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "@thunberg/eslint-config": "workspace:*",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-modules-newline": "^0.0.6",
    "rollup": "^2.58.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
