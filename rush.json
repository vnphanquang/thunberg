{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.52.0",
  "pnpmVersion": "6.19.0",
  "pnpmOptions": {
    "pnpmStore": "local",
    "strictPeerDependencies": false,
    "resolutionStrategy": "fast",
    "preventManualShrinkwrapChanges": false,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": "14.17.5",
  "suppressNodeLtsWarning": false,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 4,
  "allowMostlyStandardPackageNames": false,

  "approvedPackagesPolicy": {
    "reviewCategories": [
      "libraries",
      "vnphanquang",
      "tools"
    ]
  },

  "gitPolicy": {
    "allowedEmailRegExps": [
      "vnphanquang@gmail.com"
    ],
    "sampleEmail": "vnphanquang@gmail.com",
    "versionBumpCommitMessage": "chore(rush): apply package updates. [skip-ci]",
    "changeLogUpdateCommitMessage": "chore(rush): delete change files and updating change logs for package updates. [skip-ci]"
  },

  "repository": {
    "defaultBranch": "develop",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,

  "projects": [
    {
      "packageName": "@vnphanquang/eslint-config",
      "projectFolder": "tools/eslint-config",
      "reviewCategory": "tools",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/toolbox",
      "projectFolder": "tools/toolbox",
      "reviewCategory": "tools",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/template-lib",
      "projectFolder": "libs/__template",
      "reviewCategory": "tools",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/template-app",
      "projectFolder": "apps/__template",
      "reviewCategory": "tools",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/common",
      "projectFolder": "libs/common",
      "reviewCategory": "libraries",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/tailwind",
      "projectFolder": "libs/tailwind",
      "reviewCategory": "libraries",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/i18n",
      "projectFolder": "apps/vnphanquang/i18n",
      "reviewCategory": "vnphanquang",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/graphql",
      "projectFolder": "apps/vnphanquang/graphql",
      "reviewCategory": "vnphanquang",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    },
    {
      "packageName": "@vnphanquang/frontend",
      "projectFolder": "apps/vnphanquang/frontend",
      "reviewCategory": "vnphanquang",
      "cyclicDependencyProjects": [],
      "skipRushCheck": false,
      "shouldPublish": false
    }
  ]
}
